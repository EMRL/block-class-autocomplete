{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
